{
  "name": "audioszum",
  "version": "1.0.0",
  "description": "Szersze spojżenie na elektronikę",
  "main": "index.js",
  "pwaManifest": {
    "name": "Audioszum",
    "shortName": "AS",
    "startURL": "/",
    "theme": "#39505f",
    "generateIconOptions": {
      "baseIcon": "./src/img/icon/android-chrome-512x512.png",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "scripts": {
    "clean": "rm dist/*.*",
    "start:dev": "parcel serve src/index.pug -p 8080",
    "build:prod": "rm -rf dist && parcel build src/index.pug"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "npm": "^6.14.11",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "pug": "^3.0.0",
    "sass": "^1.32.5"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "normalize.css": "^8.0.1",
    "parcel-plugin-pwa-manifest": "^3.0.10",
    "style": "0.0.3",
    "tailwindcss": "1.9.6"
  }
}